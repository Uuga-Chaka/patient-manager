{
  "name": "history",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron.js",
  "build": {
    "appId": "uughaa.chaka.dev",
    "directories": {
      "buildResources": "assets"
    },
    "win": {
      "category": "uughaa.patient.manager",
      "iconUrl": "./LogoPersonal.png"
    },
    "mac": {
      "category": "uughaa.patient.manager",
      "iconUrl": "./LogoPersonal.png"
    }
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "electron-is-dev": "^1.1.0",
    "node-sass": "^4.13.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0"
  },
  "scripts": {
